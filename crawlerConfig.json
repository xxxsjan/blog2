{
  "index_name": "blog2",
  "start_urls": [
    "https://blog2.odep.xyz/"
  ],
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "lvl0": {
      "selector": "",
      "defaultValue": "Documentation"
    },
    "lvl1": ".theme-reco-md-content h1",
    "lvl2": ".theme-reco-md-content h2",
    "lvl3": ".theme-reco-md-content h3",
    "lvl4": ".theme-reco-md-content h4",
    "lvl5": ".theme-reco-md-content h5",
    "content": ".theme-reco-md-content p, .theme-reco-md-content li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "selectors_exclude": [
    "aside",
    ".page-footer",
    ".next-and-prev-link",
    ".table-of-contents"
  ],
  "custom_settings": {
    "attributesForFaceting": [
      "lang",
      "tags"
    ]
  },
  "js_render": true
}